{
	"description": "Utility that offers quality of life changes for the New York Times crossword puzzle.",
	"manifest_version": 2,
	"name": "NYT Crossword Helper",
	"version": "1.0",
	"homepage_url": "https://gusberinger.com/",
	"icons": {
		"48": "icons/logo-48.png"
	},

	"content_scripts": [
		{
			"matches": ["https://www.nytimes.com/crosswords/game/daily/"],
			"js": ["src/replace.js"]
		},
		{
			"matches": ["*://*.nytimes.com/crosswords/game/daily/*"],
			"js": ["src/pencil.js"]
		}
	]
}
